#!/usr/bin/env bash
# List all processes from all users, including those without TTY, with resource usage and full start time (HH:MM:SS), showing hierarchy
printf "%-10s %-8s %5s %5s %8s %8s %-8s %-5s %-9s %s\n" \
  USER PID %CPU %MEM VSZ RSS TTY STAT START COMMAND

ps -e -o user=,pid=,pcpu=,pmem=,vsz=,rss=,tty=,stat=,start=,args= --forest
